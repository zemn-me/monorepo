{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"module": "preserve",
		"moduleResolution": "bundler",
		"target": "es2020",
		"strictFunctionTypes": true,
		"esModuleInterop": true,
		"strict": true,
		"jsx": "react-jsx",
		"resolveJsonModule": true,
		"noUncheckedIndexedAccess": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"declaration": true,
		"noImplicitOverride": true,
		"forceConsistentCasingInFileNames": true,
		"downlevelIteration": true,
		"allowJs": true,
		"declarationMap": true,
		"lib": [
			"DOM",
			"ESNext"
		],
		"baseUrl": ".",
		"typeRoots": [
			"./types",
			"../../types",
			"./node_modules/@types",
			"../../node_modules/@types",
			"../../dist/bin/types"
		],
		"types": [
			"node",
			"jest"
		],
		"paths": {
			"#root/*": [
				"*",
				"dist/bin/*"
			],
			"next/dist/compiled/webpack/webpack": [
				"./types/next-compiled/index.d.ts",
				"../../types/next-compiled/index.d.ts",
				"../../dist/bin/types/next-compiled/index.d.ts"
			],
			"VAR_MODULE_GLOBAL_ERROR": [
				"./types/next-compiled/index.d.ts",
				"../../types/next-compiled/index.d.ts",
				"../../dist/bin/types/next-compiled/index.d.ts"
			],
			"react-server-dom-webpack/server.edge": [
				"./types/next-compiled/index.d.ts",
				"../../types/next-compiled/index.d.ts",
				"../../dist/bin/types/next-compiled/index.d.ts"
			],
			"next/dist/compiled/superstruct": [
				"./types/next-compiled/index.d.ts",
				"../../types/next-compiled/index.d.ts",
				"../../dist/bin/types/next-compiled/index.d.ts"
			],
			"uuid": [
				"./types/uuid",
				"../../types/uuid",
				"../../dist/bin/types/uuid"
			]
		}
	},
	"exclude": [
		"node_modules",
		"dist",
		"external",
		".venv",
		".script_cache",
		".sl",
		".git"
	]
}
